{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["index.html", "src/main.tsx"],
  "project": ["**/*.{ts,tsx}"],
  "ignore": [
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/__tests__/**",
    "dist/**",
    "build/**",
    "coverage/**",
    "node_modules/**",
    "public/**"
  ],
  "ignoreDependencies": ["@types/*"],
  "ignoreBinaries": ["vite", "tsc", "eslint", "vitest"],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": false,
  "rules": {
    "files": "error",
    "dependencies": "error",
    "devDependencies": "warn",
    "unlisted": "error",
    "exports": "warn",
    "types": "warn",
    "nsExports": "error",
    "nsTypes": "error",
    "duplicates": "error",
    "enumMembers": "warn"
  }
}
