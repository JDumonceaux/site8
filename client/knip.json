{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["index.html", "src/main.tsx"],
  "project": ["**/*.{ts,tsx}"],
  "ignore": [
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/__tests__/**",
    "dist/**",
    "build/**",
    "coverage/**",
    "node_modules/**",
    "public/**"
  ]
}
