{
  "name": "react-ts",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm run type-check && vite build",
    "build:analyze": "cross-env NODE_ENV=production npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "dev": "vite",
    "fix": "npm run lint && npm run prettier",
    "knip": "knip --files --exports",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "prepare": "husky",
    "prepare-commit-msg": "sh tools/git-commit-message.sh .git/COMMIT_EDITMSG",
    "precommit": "pretty-quick --staged",
    "preview": "vite preview",
    "serve": "vite preview",
    "start": "vite",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.12.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@radix-ui/react-accessible-icon": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-form": "^0.1.8",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@reduxjs/toolkit": "^2.9.0",
    "@tanstack/react-query": "^5.87.4",
    "aws-amplify": "^6.15.6",
    "axios": "^1.12.1",
    "cross-spawn": "^7.0.6",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "html-react-parser": "^5.2.6",
    "i18next": "^25.5.2",
    "memoize-one": "^6.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^16.0.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.1",
    "react-scan": "^0.4.3",
    "react-window": "^2.1.0",
    "styled-components": "^6.1.19",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.35.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@tanstack/eslint-plugin-query": "^5.86.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/amplify": "^1.1.28",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.3.3",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-jest": "^30.1.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-redux": "^4.2.2",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.5.0",
    "eslint-plugin-storybook": "^9.1.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "knip": "^5.63.1",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "react-scan": {
      "esbuild": "0.25.1"
    },
    "react-ts": {
      "storybook": "$storybook"
    },
    "@xstate/react@3.2.2": {
      "react": "$react"
    },
    "@typescript-eslint/typescript-estree@8.19.1": {
      "typescript": "$typescript"
    },
    "@typescript-eslint/utils@8.19.1": {
      "typescript": "$typescript"
    },
    "react-confetti@6.1.0": {
      "react": "$react"
    },
    "esbuild": "0.25.1"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "last 2 versions",
    "maintained node versions"
  ],
  "packageManager": "pnpm@9.6.0",
  "license": "UNLICENSED"
}
