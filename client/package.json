{
  "name": "react-ts",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite preview",
    "build": "npm run type-check && vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src\" --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --ignore-path .prettierignore",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "pre-commit": "pretty-quick --staged",
    "prepare-commit-msg": "sh tools/git-commit-message.sh .git/COMMIT_EDITMSG",
    "upgrade": "pnpm up --latest --interactive",
    "clean": "rimraf dist coverage",
    "knip": "knip --debug"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.13.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@radix-ui/react-accessible-icon": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-form": "^0.1.8",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@reduxjs/toolkit": "^2.11.2",
    "@site8/shared": "file:../shared",
    "@tanstack/react-query": "^5.90.16",
    "@vitejs/plugin-react": "^5.1.2",
    "aws-amplify": "^6.15.9",
    "date-fns": "^4.1.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "html-react-parser": "^5.2.11",
    "i18next": "^25.7.3",
    "react": "^19.2.3",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.1",
    "react-i18next": "^16.5.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.11.0",
    "react-scan": "^0.4.3",
    "styled-components": "^6.1.19",
    "web-vitals": "^5.1.0",
    "zod": "^4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/react": "^10.1.11",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/amplify": "^1.1.28",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitest/browser": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-perfectionist": "^5.2.0",
    "eslint-plugin-prefer-arrow-functions": "^3.9.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-perf": "^3.3.3",
    "eslint-plugin-react-redux": "^4.2.2",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.8.3",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-storybook": "^10.1.11",
    "eslint-plugin-testing-library": "^7.15.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "knip": "^5.78.0",
    "prettier": "^3.7.4",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-bundle-analyzer": "^1.3.2",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "react-scan": {
      "esbuild": "0.25.1"
    },
    "@xstate/react@3.2.2": {
      "react": "$react"
    },
    "@typescript-eslint/typescript-estree@8.19.1": {
      "typescript": "$typescript"
    },
    "@typescript-eslint/utils@8.19.1": {
      "typescript": "$typescript"
    },
    "esbuild": "0.25.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "last 2 versions",
    "maintained node versions"
  ],
  "sideEffects": false,
  "packageManager": "npm@11.6.2",
  "license": "UNLICENSED"
}
