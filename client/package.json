{
  "name": "react-ts",
  "version": "1.0.0",
  "type": "module",
  "module": "dist/index",
  "browser": "dist/index",
  "main": "dist/index",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "set NODE_ENV=development tsc && vite build",
    "build:prod": "set NODE_ENV=production tsc && vite build",
    "build:analyze": "set NODE_ENV=production tsc && vite build",
    "fix": "npm run lint && npm run prettier",
    "precommit": "pretty-quick --staged",
    "preview": "vite preview",
    "serve": "vite preview",
    "lint": "eslint --inspect-config",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "type-check": "tsc",
    "prepare": "husky",
    "pre-commit": "npx lint-staged",
    "prepare-commit-msg": "sh tools/git-commit-message.sh .git/COMMIT_EDITMSG",
    "knip": "knip --files --exports",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest"
  },
