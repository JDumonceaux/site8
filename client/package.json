    "prepare": "husky",
    "pre-commit": "npx lint-staged",
    "prepare-commit-msg": "sh tools/git-commit-message.sh .git/COMMIT_EDITMSG",
    "knip": "knip --production",
    "storybook": "storybook dev -p 6006",
