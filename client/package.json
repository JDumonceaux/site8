  "overrides": {
    "react-scan": {
      "esbuild": "0.25.1"
    },
    "@xstate/react@3.2.2": {
      "react": "$react"
    },
    "@typescript-eslint/typescript-estree@8.19.1": {
      "typescript": "$typescript"
    },
    "@typescript-eslint/utils@8.19.1": {
      "typescript": "$typescript"
    },
    "esbuild": "0.25.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "last 2 versions",
    "maintained node versions"
  ],
  "sideEffects": false,
  "packageManager": "npm@11.6.2",
  "license": "UNLICENSED"
