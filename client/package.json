    "@reduxjs/toolkit": "^2.9.0",
    "@tanstack/react-query": "^5.90.2",
    "@vitejs/plugin-react": "^5.0.4",
    "aws-amplify": "^6.15.7",
    "axios": "^1.12.2",
    "cross-spawn": "^7.0.6",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "html-react-parser": "^5.2.7",
    "react": "^19.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^16.1.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
