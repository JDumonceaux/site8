{
  "name": "server1",
  "version": "0.1.0",
  "description": "API service for the application",
  "type": "module",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "debug": "npm run build && node --inspect dist/server.js",
    "build": "tsc",
    "precommit": "pretty-quick --staged",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "pretty-quick --staged",
    "prepare": "husky",
    "prepare-commit-msg": "node tools/git-commit-message.js .git/COMMIT_EDITMSG",
    "knip": "knip --production"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "validator": "^13.15.15",
    "winston": "^3.18.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@types/validator": "^13.15.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "knip": "^5.64.1",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.2.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  },
  "keywords": [
    "api",
    "express",
    "typescript",
    "nodejs"
  ],
  "author": "John Doe <john.doe@example.com>",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/username/repository.git"
  },
  "bugs": {
    "url": "https://github.com/username/repository/issues"
  },
  "homepage": "https://github.com/username/repository#readme"
