{
  "name": "server1",
  "version": "0.1.0",
  "description": "API service for the application",
  "type": "module",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "debug": "npm run build && node --inspect dist/server.js",
    "build": "tsc",
    "precommit": "pretty-quick --staged",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "prepare": "husky",
    "prepare-commit-msg": "sh tools/git-commit-message.sh .git/COMMIT_EDITMSG",
    "knip": "knip --production"
  },
  "dependencies": {
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "validator": "^13.12.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/validator": "^13.12.1",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.9.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^57.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "knip": "^5.46.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "keywords": [
    "api",
    "express",
    "typescript",
    "nodejs"
  ],
  "author": "John Doe <john.doe@example.com>",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/username/repository.git"
  },
  "bugs": {
    "url": "https://github.com/username/repository/issues"
  },
  "homepage": "https://github.com/username/repository#readme"
}
