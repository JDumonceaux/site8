{
  "name": "server1",
  "version": "0.1.0",
  "description": "API service for the application",
  "type": "module",
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "debug": "npm run build && node --inspect dist/server.js",
    "build": "tsc",
    "build:clean": "rm -rf dist && tsc",
    "precommit": "pretty-quick --staged",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "pretty-quick --staged",
    "prepare": "husky",
    "prepare-commit-msg": "node tools/git-commit-message.js .git/COMMIT_EDITMSG",
    "knip": "knip",
    "start:prod": "NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@site8/shared": "file:../shared",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "validator": "^13.15.26",
    "winston": "^3.19.0",
    "zod": "^4.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/validator": "^13.15.10",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-perfectionist": "^5.2.0",
    "eslint-plugin-prefer-arrow-functions": "^3.9.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "knip": "^5.78.0",
    "prettier": "^3.7.4",
    "pretty-quick": "^4.2.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "keywords": [
    "api",
    "express",
    "typescript",
    "nodejs"
  ],
  "author": "John Doe <john.doe@example.com>",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/username/repository.git"
  },
  "bugs": {
    "url": "https://github.com/username/repository/issues"
  },
  "homepage": "https://github.com/username/repository#readme",
  "dev": "tsx watch src/server.ts",
  "typecheck": "tsc --noEmit",
  "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
  "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
  "tsx": "^4.19.2",
  "jest": "^30.0.0",
  "ts-jest": "^30.0.0"
}
