{
  "name": "server1",
  "version": "0.1.0",
  "description": "API service for the application",
  "type": "module",
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "debug": "npm run build && node --inspect dist/server.js",
    "build": "tsc",
    "build:clean": "rm -rf dist && tsc",
    "precommit": "pretty-quick --staged",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "pretty-quick --staged",
    "prepare": "husky",
    "prepare-commit-msg": "node tools/git-commit-message.js .git/COMMIT_EDITMSG",
    "knip": "knip",
    "knip:prod": "knip --production",
    "start:prod": "NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "validator": "^13.15.15",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.8.1",
    "@types/validator": "^13.15.3",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prefer-arrow-functions": "^3.9.1",
    "eslint-plugin-promise": "^6.6.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "knip": "^5.66.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.2.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "keywords": [
    "api",
    "express",
    "typescript",
    "nodejs"
  ],
  "author": "John Doe <john.doe@example.com>",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/username/repository.git"
  },
  "bugs": {
    "url": "https://github.com/username/repository/issues"
  },
  "homepage": "https://github.com/username/repository#readme",
  "dev": "tsx watch src/server.ts",
  "typecheck": "tsc --noEmit",
  "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
  "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
  "tsx": "^4.19.2",
  "jest": "^30.0.0",
  "ts-jest": "^30.0.0"
}
